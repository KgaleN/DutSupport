@model IEnumerable<Support.Models.SupportEmployee>

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Type</th>
            <th scope="col">Id Number</th>
            <th scope="col">Email</th>
            <th scope="col">Name</th>
            <th scope="col">Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <form asp-action="SupportEmployeeStatistics">
                <input type="hidden" name="supportId" value="@item.Id" />
            <tr class="table-active">
                <th scope="row">Active</th>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Role</td>
                <td><button class="btn btn-success assign-button" type="submit">View Details</button></td>
            </tr>
            
           
                
            </form>
        
        }
    </tbody>
</table>